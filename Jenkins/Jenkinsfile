pipeline {
        agent any
	stages {
	       stage('stage1'){
	                steps {
			      sh ' echo "build successful from github" '
			      }
			      }
	       stage('stage2'){
	                 steps{
			      sh '''
			           for i in {1..10}
				   do
				       Echo "build running on `hostname`"
				  '''
			       }
			       }

	 }      
